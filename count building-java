import java.util.*;
class Building
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n;
n=sc.nextInt();

int a[]=new int[n];
for(int i=0;i<n;i++)
   {
      a[i]=sc.nextInt();
    }

int c=1;
int s1=a[0];
for(int i=0;i<n;i++)
{
if(s1<a[i])
{
c++;
}
}
System.out.println(c);
}
}
