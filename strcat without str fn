#include <stdio.h>
int main() {
    char str1[50],str2[50],str3[20];
    int i,j;
    scanf("%s\n%s",str1,str2);
   for(i=0;str1[i]!='\0';i++)
   {
       str3[i]=str1[i];
  }
   for(j=0;str2[j]!='\0';j++)
   {
       //scanf("%s\n%s",str1[i],str2[j]);
       str3[i+j]=str2[j];
   }
   str3[i+j]='\0';
   printf("%s",str3);    

    return 0;
}
