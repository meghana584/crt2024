import java.util.*;

class Megha
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int n,i,c=0,r=0;
n=s.nextInt();
for(i=1;i<=n;i++)
{
if(n==i)

c=1;

else
{
for( i=2;i<=n/2;i++)

r++;
if(n%i==0)

c++;
break;
}
}

System.out.println(r);
if(c==2)
System.out.println("Prime no");
else
System.out.println("Not a Prime");
}
