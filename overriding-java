import java .util.*;
class Father
{
    void surName()
         {
        System.out.print("k");
          }
       void designation()
         {
        System.out.println("clerk");
         }
}
class Child extends Father
{
    void getName()
{
    System.out.println("satish");
}
void designation()
{
    System.out.println("java programmer");
}
}
class Test
{
    public static void main(String[] args) {
        Child c1=new Child();
        c1.getName();
        c1.surName();
        c1.designation();
    }
}
